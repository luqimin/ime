{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "no-eval": false,
        "no-console": false,
        "no-empty": false,
        "max-line-length": false,
        "quotemark": false,
        "eofline": false,
        "only-arrow-functions": false,
        "variable-name": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-string-literal": false,
        "no-unused-expression": false,
        "no-namespace": false,
        "one-variable-per-declaration": false,
        "object-literal-shorthand": false,
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "ignore",
                    "typeLiterals": "ignore"
                },
                "singleline": "never",
                "esSpecCompliant": true
            }
        ],
        "space-before-function-paren": [
            true,
            {
                "anonymous": "always",
                "named": "never",
                "asyncArrow": "always"
            }
        ],
        "prefer-const": [
            true,
            {
                "destructuring": "all"
            }
        ],
        "radix": false,
        "forin": false,
        "interface-name": false
    },
    "rulesDirectory": []
}
